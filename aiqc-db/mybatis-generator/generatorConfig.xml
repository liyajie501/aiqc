        <sqlMapGenerator targetPackage="com.sinosoft.aiqc.db.dao" targetProject="src/main/resources"/>
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.sinosoft.aiqc.db.dao"
                             targetProject="src/main/java"/>
